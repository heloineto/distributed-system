    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "date-fns": "^2.25.0",
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "final-form": "^4.20.4",
    "lodash": "^4.17.21",
    "mui-rff": "^3.0.16",
    "notistack": "^1.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.7"
